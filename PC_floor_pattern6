<style>
#floor${floor_no} .lcltr_cur span{
    border-bottom: 2px solid #${floor_tit_bg};
}
#floor${floor_no} .lcrt_title li.active{
    margin-bottom: 0;
    border-bottom: 3px solid #${floor_tit_bg};
}
</style>
<script>

$(function(){

  $('#floor${floor_no} .lclt_right').find(' li').mouseover(function(){
    var active_num = $(this).index();
    $(this).find('span').remove();
    $(this).append('<span><em></em></span>');
    $(this).addClass('lcltr_cur').siblings().removeClass('lcltr_cur');
    $('#floor${floor_no} .lcl_bottom_cms').eq(active_num).show().siblings('.lcl_bottom_cms').hide();o
	 $('#floor${floor_no} .lcl_bottom_cms').eq(active_num).find('img.lazy').each(function(){
          $(this).show().lazyload();
    })

  })

	/*if(${floor_no}=='1F'){ */
	$('.lclb_right').each(function(i){
			  $(this).show().find('img.lazy').lazyload();
			if(i==0)
				$(this).show();
			else
				$(this).hide();
	  })
	/*}*/
  /*
  $('#floor${floor_no} .lcrt_bottom').each(function(){
      $(this).find('li').eq(0).css('border-left','none');
      $(this).find('li').eq(2).css('border-left','none');
      $(this).find('li').eq(4).css('border-left','none');
      $(this).find('li').eq(6).css('border-left','none');
  })
  */
})

</script>
